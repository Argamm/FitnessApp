<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/nav_main"
    app:startDestination="@id/descriptionFragment">

    <fragment
        android:id="@+id/descriptionFragment"
        android:name="com.example.fitnessapp.fragment.DescriptionFragment"
        android:label="DescriptionFragment" >
        <action
            android:id="@+id/action_descriptionFragment_to_loginFragment"
            app:destination="@id/loginFragment" />
    </fragment>
    <fragment
        android:id="@+id/selectGenderFragment"
        android:name="com.example.fitnessapp.fragment.SelectGenderFragment"
        android:label="SelectGenderFragment" >
        <action
            android:id="@+id/action_selectGenderFragment_to_editInformationFragment"
            app:destination="@id/editInformationFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_enter_anim"
            app:popExitAnim="@anim/nav_default_exit_anim"/>
    </fragment>
    <fragment
        android:id="@+id/editInformationFragment"
        android:name="com.example.fitnessapp.fragment.EditInformationFragment"
        android:label="EditInformationFragment" >
        <action
            android:id="@+id/action_editInformationFragment_to_homeFragment2"
            app:destination="@id/homeFragment2"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_enter_anim"
            app:popExitAnim="@anim/nav_default_exit_anim"/>
    </fragment>
    <fragment
        android:id="@+id/homeFragment2"
        android:name="com.example.fitnessapp.fragment.navigation.HomeFragment"
        android:label="HomeFragment" >
        <action
            android:id="@+id/action_homeFragment2_to_myDialogFragment"
            app:destination="@id/myDialogFragment" />
    </fragment>
    <fragment
        android:id="@+id/analyticsFragment2"
        android:name="com.example.fitnessapp.fragment.navigation.AnalyticsFragment"
        android:label="AnalyticsFragment" />
    <fragment
        android:id="@+id/customTrainingsFragment2"
        android:name="com.example.fitnessapp.fragment.navigation.CustomTrainingsFragment"
        android:label="CustomTrainingsFragment" >
        <action
            android:id="@+id/action_customTrainingsFragment2_to_tasksFragment"
            app:destination="@id/tasksFragment" />
    </fragment>
    <fragment
        android:id="@+id/myProfileFragmentNav"
        android:name="com.example.fitnessapp.fragment.navigation.MyProfileFragment"
        android:label="MyProfileFragment" >
        <action
            android:id="@+id/action_myProfileFragmentNav_to_feedbackFragment"
            app:destination="@id/feedbackFragment"
            app:enterAnim="@animator/nav_default_enter_anim"
            app:exitAnim="@animator/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim"/>
        <action
            android:id="@+id/action_myProfileFragmentNav_to_mineProfileFragment"
            app:destination="@id/mineProfileFragment"
            app:enterAnim="@animator/nav_default_enter_anim"
            app:exitAnim="@animator/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim"/>
        <action
            android:id="@+id/action_myProfileFragmentNav_to_communicateFragment"
            app:destination="@id/communicateFragment"
            app:enterAnim="@animator/nav_default_enter_anim"
            app:exitAnim="@animator/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim"/>
        <action
            android:id="@+id/action_myProfileFragmentNav_to_rateUsFragment"
            app:destination="@id/rateUsFragment"
            app:enterAnim="@animator/nav_default_enter_anim"
            app:exitAnim="@animator/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim"/>
        <action
            android:id="@+id/action_myProfileFragmentNav_to_remainderFragment"
            app:destination="@id/remainderFragment"
            app:enterAnim="@animator/nav_default_enter_anim"
            app:exitAnim="@animator/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim"/>
        <action
            android:id="@+id/action_myProfileFragmentNav_to_languageFragment"
            app:destination="@id/languageFragment" />
    </fragment>
    <fragment
        android:id="@+id/trainingsFragment2"
        android:name="com.example.fitnessapp.fragment.navigation.TrainingsFragment"
        android:label="TrainingsFragment" >
        <action
            android:id="@+id/action_trainingsFragment2_to_armsFragment"
            app:destination="@id/armsFragment" />
        <action
            android:id="@+id/action_trainingsFragment2_to_legsFragment"
            app:destination="@id/legsFragment" />
        <action
            android:id="@+id/action_trainingsFragment2_to_shortTrainingsFragment"
            app:destination="@id/shortTrainingsFragment" />
        <action
            android:id="@+id/action_trainingsFragment2_to_armsWorkoutPlaneFragment"
            app:destination="@id/armsWorkoutPlaneFragment" />
    </fragment>
    <fragment
        android:id="@+id/armsFragment"
        android:name="com.example.fitnessapp.fragment.navigation.trainings.ArmsFragment"
        android:label="ArmsFragment" >
        <action
            android:id="@+id/action_armsFragment_to_armsWorkoutPlaneFragment"
            app:destination="@id/armsWorkoutPlaneFragment" />
    </fragment>
    <fragment
        android:id="@+id/legsFragment"
        android:name="com.example.fitnessapp.fragment.navigation.trainings.LegsFragment"
        android:label="LegsFragment" />
    <fragment
        android:id="@+id/shortTrainingsFragment"
        android:name="com.example.fitnessapp.fragment.navigation.trainings.ShortTrainingsFragment"
        android:label="ShortTrainingsFragment" />
    <fragment
        android:id="@+id/feedbackFragment"
        android:name="com.example.fitnessapp.fragment.navigation.myProfile.FeedbackFragment"
        android:label="FeedbackFragment" />
    <fragment
        android:id="@+id/mineProfileFragment"
        android:name="com.example.fitnessapp.fragment.navigation.myProfile.MineProfileFragment"
        android:label="MineProfileFragment" />
    <fragment
        android:id="@+id/communicateFragment"
        android:name="com.example.fitnessapp.fragment.navigation.myProfile.CommunicateFragment"
        android:label="CommunicateFragment" />
    <fragment
        android:id="@+id/rateUsFragment"
        android:name="com.example.fitnessapp.fragment.navigation.myProfile.RateUsFragment"
        android:label="RateUsFragment" />
    <fragment
        android:id="@+id/remainderFragment"
        android:name="com.example.fitnessapp.fragment.navigation.myProfile.RemainderFragment"
        android:label="RemainderFragment" />
    <fragment
        android:id="@+id/ABSFragment"
        android:name="com.example.fitnessapp.fragment.navigation.trainings.ABSFragment"
        android:label="ABSFragment" />
    <fragment
        android:id="@+id/tasksFragment"
        android:name="com.example.fitnessapp.fragment.navigation.customTrainings.TasksFragment"
        android:label="TasksFragment" />
    <dialog
        android:id="@+id/myDialogFragment"
        android:name="com.example.fitnessapp.fragment.navigation.home.MyDialogFragment"
        android:label="MyDialogFragment" />
    <fragment
        android:id="@+id/loginFragment"
        android:name="com.example.fitnessapp.fragment.LoginFragment"
        android:label="LoginFragment" >
        <action
            android:id="@+id/action_loginFragment_to_homeFragment2"
            app:destination="@id/homeFragment2" />
        <action
            android:id="@+id/action_loginFragment_to_selectGenderFragment"
            app:destination="@id/selectGenderFragment" />
    </fragment>
    <fragment
        android:id="@+id/languageFragment"
        android:name="com.example.fitnessapp.fragment.navigation.myProfile.LanguageFragment"
        android:label="LanguageFragment" />
    <fragment
        android:id="@+id/armsWorkoutPlaneFragment"
        android:name="com.example.fitnessapp.fragment.navigation.trainings.arms.ArmsWorkoutPlaneFragment"
        android:label="ArmsWorkoutPlaneFragment" >
        <argument
            android:name="list"
            app:argType="integer[]"
            app:nullable="true" />
    </fragment>
</navigation>